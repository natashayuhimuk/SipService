<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_5.xsd"
             version="5">
    <display-name>SipService</display-name>

    <module>
        <ejb>logger-0.1.jar</ejb>
    </module>
    <module>
        <ejb>dao-ejb-0.1.jar</ejb>
    </module>
    <module>
        <ejb>trafficlogic-0.1.jar</ejb>
    </module>
    <module>
        <ejb>statistics-0.1.jar</ejb>
    </module>
    <module>
        <web>
            <web-uri>sipcontroller-0.1.war</web-uri>
            <context-root>/sipcontroller</context-root>
        </web>
    </module>
    <module>
        <web>
            <web-uri>webservices-0.1.war</web-uri>
            <context-root>/webservices</context-root>
        </web>
    </module>

</application>
